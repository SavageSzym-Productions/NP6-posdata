// Felipe Ramas - NVS-4118 - Implement Table Service Model Specifications
function VerifyAndShowTagIdSmartScreenJS(firstSmart,saleType) 
{ 

	/* var hlp = new BusinessObjectHelper;  */
	var cytTagIdSR = rootHlp.findParamInSectionConfig("cytTagIdSR","UserInterface");
	var tableServiceEatIn = rootHlp.findParamInSectionConfig("tableService.eatIn","UserInterface");
	var tableServiceTakeOut = rootHlp.findParamInSectionConfig("tableService.takeOut","UserInterface");
	var hasCYTInSale = PosHasCYTProductInSale(true);
	var displayTagIdFirst = PosCheckParameter("SmartRouting", "displayTagIdFirst", "true"); 
	var orderSource=rootHlp.GetOrderSrc();
	if (tableServiceEatIn == null || tableServiceEatIn == "") tableServiceEatIn = "ONLY_WITH_CYT";
	if (tableServiceTakeOut == null || tableServiceTakeOut == "") tableServiceTakeOut = "ONLY_WITH_CYT";

	/*NVS-6925 - msilva - Check Tag ID conditions*/
	var tagIDMatchCondition = false;

	//NVS-6854_NVS-6866 - msilva - Do not ask for Tag Id originated Kiosk orders with previous selected one.
	var viewXml = rootHlp.getCurrentView();
	var view = new XML(viewXml);	
	var isKiosk = (view.@pod == PodDefinitions.CSO);
	var currentTagId = view.@tableTagId;
	var hasTagId = (currentTagId != undefined && currentTagId != null && currentTagId != "");
	var tagIdRemoved = rootCtx.get("TagIdRemoved");
	
	if (tagIdRemoved == undefined || tagIdRemoved == null || tagIdRemoved == "") {
		rootCtx.set("TagIdRemoved", false);
		tagIdRemoved = false;
	}
	
	
	if (cytTagIdSR != null && cytTagIdSR != "") //NVS-6482 Mobile Order do not need Tag ID
	{ 
		// EATIN
		if ((saleType == 0) && (tableServiceEatIn != "NEVER")) {
			if ((hasCYTInSale) || (tableServiceEatIn == "ALWAYS")) {

				/*NVS-6925 - msilva - Match Tag ID conditions*/				
				tagIDMatchCondition = true;

				if(orderSource == 1 && (!hasTagId || view.@tableTagId == "-1")){//NVS-9480, mobile order need tagID

					return(PosShowTagIdSmartScreen(cytTagIdSR));
				}
				if(((displayTagIdFirst) && (firstSmart)) || ((!displayTagIdFirst) && (!firstSmart))) { 
					//No table service selected at Kiosk until the moment the order changes tag ID matching conditions
					if(isKiosk && !hasTagId ){
						if (tagIdRemoved != undefined && tagIdRemoved != null && (tagIdRemoved == false || tagIdRemoved == "false")) {
							//Bypass tag Id not selected in the Kiosk (No Table Service)
							return false;
						}
					}
					if(orderSource == 1 && !tagIdRemoved){
						return false;
					}

					return(PosShowTagIdSmartScreen(cytTagIdSR));
				}			
			}	
		} 	
		// TAKEOUT
		if ((saleType == 1) && (tableServiceTakeOut != "NEVER")) {
			if ((hasCYTInSale) || (tableServiceTakeOut == "ALWAYS")) {

				/*NVS-6925 - msilva - Match Tag ID conditions*/
				tagIDMatchCondition = true;
					
				if(((displayTagIdFirst) && (firstSmart)) || ((!displayTagIdFirst) && (!firstSmart))) { 

					//No table service selected at Kiosk until the moment the order changes tag ID matching conditions
					if(isKiosk && !hasTagId ){
						if (tagIdRemoved != undefined && tagIdRemoved != null && (tagIdRemoved == false || tagIdRemoved == "false")) {
							//Bypass tag Id not selected in the Kiosk (No Table Service)
							return false;
						}
					}
					if(orderSource == 1 && !tagIdRemoved){
						return false;
					}
					
					return(PosShowTagIdSmartScreen(cytTagIdSR));
				}			
			}	
		} 	
	  
		/*

		if(PosHasCYTProductInSale(true) && PosVerifyTagId()) 
		{ 
			var displayTagIdFirst = PosCheckParameter("SmartRouting", "displayTagIdFirst", "true"); 
			if((displayTagIdFirst) && (firstSmart)) 
			{ 
				 return(PosShowTagIdSmartScreen(cytTagIdSR)); 
			} 
			if((!displayTagIdFirst) && (!firstSmart)) 
			{ 
				 return(PosShowTagIdSmartScreen(cytTagIdSR)); 
			} 
		} 

		*/
	} 
	
	if(orderSource == 1){
		if(currentTagId <= 0){
			if(currentTagId == "-3" || !hasTagId){
				tagIdRemoved = false;
			}else{
				PosRemoveTagID();
				tagIdRemoved = true;
			}
		}
	}
	/*NVS-6925 - msilva - Clear tag ID for not matched Conditions*/
	if(!tagIDMatchCondition)
	{
		PosRemoveTagID();
	}

	return(false); 
}

