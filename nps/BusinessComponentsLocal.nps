		var view = new XML(viewXml);
		var isPromoOrder = (view.@isPromoOrder == "true"); //The helper isPromoOrder is not working at this time
		//Sending apppropriate command
		if(isPromoOrder)
		{
			ret = PosDoSendCommandToFOEJS(MOBILE_PAYMENT_CANCEL); 
		}
		else
		{
			ret = PosDoSendCommandToFOEJS(MOBILE_PAYMENT_EXECUTE); 
		}
		
		if(!ret) 
		{ 
			PosShowMessage('MSG_BC_MOBILE_REQUEST_FAILED'); 
		} 
		else 
		{ 
			//NVS-4801 - msilva
			PosSetSessionProperty("SavedFOEView", "", "true");	// NPS-12260 

			//NVS-5944 - msilva - Avoid displaying the Store screen on the Cashier/Both after the crew clicks on Pay by Mobile button
			if(PosCheckSessionProperty("workingMode","orderTaker"))
			{
				PosShowScreen(STORE_SCREEN_NBR); 
			}
			else
			{
				PosShowScreen(RECALL_MOBILE_ORDER);
				if(rootHlp.GetMobileOrderStatus() == MOS_PENDING)
				{
					PosDoSetGenericCounter(true,30);
				}
				else
				{
					PosDoSetGenericCounter(false,0);
				}
			}

		} 
		PosDoAcceptMobileOrderJS('false','false','false','true'); // added parameter for NVS-1508
		PosRefreshButtons(); 
	} 
	else { 
		//NVS-4803 - msilva
		ret = true; 
		
		PosDoMobilePayment(); 
		
		PosDoSetCustomInfo("isPaidMobileOrder", true);
		var bEndOfSale = PosDoTenderJS(-1, 0, 'NOPREVIEW|SAVE'); //NVS-6406 - For mobile ordering, when the 1st order failed to apply offer(Offer expires or item doesn't meet condition)and tender this order, there will be an error message pop up about offer for the next new mobile order recall on POS.
		if (bEndOfSale) { 
			// Clear offers area 
			PosOffersClearMessage(); 
			PosOffersEndOfSale(); //NVS-6548
			PosDoEndOfSaleNoDrawer(true); //NVS-6305 - GMA-Mobile Ordering-US QA [C0_P1][SP8] Mobile Ordering: Incorrect Order Number is displayed in "Reprint Receipt" page for orders paid through ‘Pay By Mobile’ and showing correctly for ‘Normal Orders’. 
			PosShowScreen(rootCtx.get("baseScreenId")); 
		} 
		else { 
			PosDoSetCustomInfo("isPaidMobileOrder", false);
			//NVS-5959 - msilva - Mobile Order payment has failed, display the Payment Status Screen
			//PosShowScreen(RECALL_MOBILE_ORDER); 
			//NVS-6041 - msilva - Mobile Order payment has failed, display the Tender Screen on FC
			PosShowScreen(TENDER_SCREEN_NBR);
		} 
	} 
	
	return ret; 	
	
}

function PosDoSendCommandToFOEJS(command)
{
	var ret = false;
	ret = PosDoSendCommandToFOE(command);
	if(ret)
	{
		var MobileOrderStatus = MOS_NONE;
		switch(command)
		{
		case MOBILE_PAYMENT_EXECUTE:
			MobileOrderStatus = MOS_PENDING;
			break;
		case MOBILE_PAYMENT_CANCEL:
			MobileOrderStatus = MOS_DECLINED;
			break;
		case MOBILE_PAYMENT_TIMEDOUT:
			MobileOrderStatus = MOS_TIMEDOUT;
			break;
		}
		PosSetMobileOrderStatus(MobileOrderStatus);
	}
	
	return ret;
}

