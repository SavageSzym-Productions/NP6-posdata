function rSleep(seconds)
{
	var start = new NPDate();
  var end = start;
  end.setMilliseconds((seconds * 1000) + start.getMilliseconds());
  while (new NPDate() < end);
}

function base64_decode(data) 
{
  var b64 = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/=';
  var o1;
  var o2;
  var o3;
  var h1;
  var h2;
  var h3;
  var h4;
  var bits
  var i = 0;
  var  ac = 0 ;
  var  dec = '';
  var  tmp_arr = [];

  if (!data) {
    return data;
  }

  data += '';

  do { // unpack four hexets into three octets using index points in b64
    h1 = b64.indexOf(data.charAt(i++));
    h2 = b64.indexOf(data.charAt(i++));
    h3 = b64.indexOf(data.charAt(i++));
    h4 = b64.indexOf(data.charAt(i++));

    bits = h1 << 18 | h2 << 12 | h3 << 6 | h4;

    o1 = bits >> 16 & 0xff;
    o2 = bits >> 8 & 0xff;
    o3 = bits & 0xff;

    if (h3 == 64) {
      tmp_arr[ac++] = String.fromCharCode(o1);
    } else if (h4 == 64) {
      tmp_arr[ac++] = String.fromCharCode(o1, o2);
    } else {
      tmp_arr[ac++] = String.fromCharCode(o1, o2, o3);
    }
  } while (i < data.length);

  dec = tmp_arr.join('');

  return dec.replace(/\0+$/, '');
}

function GenerateDetailedSosData(pod)
{
	//Getting detailed SOS
	var businessDay = new XML(hlp.getBusinessDate("false"));
	var rVal = PosHttpRequest("POST", "<?xml version='1.0'?><methodCall><methodName>datarequest</methodName><params><param><value><string>DetailedSOS</string></value></param><param><value><string>" + businessDay + "</string></value></param><param><value><string></string></value></param><param><value><string>" + pod + "</string></value></param><param><value><string></string></value></param></params></methodCall>", "", "","text/xml"); 
	if(rVal != null)
	{
		rVal = rVal.replace("<?xml","<!-- bug_336551");
		rVal = rVal.replace("?>","bug_336551 -->");

		var xmlResponse = new XML(rVal);
		var queryId = xmlResponse.params.param.value.struct.member.(name=="id").value.i4;

		rSleep(4);

		rVal = PosHttpRequest("GET", "<?xml version='1.0'?><methodCall><methodName>Query</methodName><params><param><value><i4>" + queryId + "</i4></value></param></params></methodCall>", "", "","text/xml"); 
		rVal = rVal.replace("<?xml","<!-- bug_336551");
		rVal = rVal.replace("?>","bug_336551 -->");

		var xmlQueryResponse = new XML(rVal);
		var xmlString = base64_decode(xmlQueryResponse.params.param.value.struct.member.(name=="payload").value.base64);

		//check if Waystation returned error
		if(xmlQueryResponse.params.param.value.struct.member.(name=="faultString").value.string.toString().length > 0)
		{
			API.dbg("SOS report: error while receiving DetailedSOS from Waystation -  " + xmlQueryResponse.params.param.value.struct.member.(name=="faultString").value.string.toString());
		}
		else
		{
			xmlString = xmlString.replace("<?xml","<!-- bug_336551");
			xmlString = xmlString.replace("?>","bug_336551 -->");
			GenerateSegmentData(null, [xmlString]);
		}


    //NVS-9092
		var hourIncrementString = "<HourIncrement>";

    for (var hr = 0; hr < 24; hr++) {
      var hourIncrement = PosGetSOSIncrement("Hour" + String(hr) + "Increment");            
      hourIncrementString += "<increment hour='" + String(hr) + "' increment='" + String(hourIncrement) + "' />";
    }   
    hourIncrementString += "</HourIncrement></DetailedSOS>";

    xmlString = xmlString.replace("</DetailedSOS>",hourIncrementString);

    return xmlString;
	}
	// Communication to Waystation failed
	else
	{ 
		API.dbg("SOS report: error while receiving DetailedSOS data from Waystation - communication failed ");
	}
}


