/**PosShouldPrintSecondCopyOfReceipt
 *
 * @brief - This BC check if it is necessary to print a second copy of the receipt
 * Return - true if the second copy of the receipt should be printed
 */
function PosShouldPrintSecondCopyOfReceipt() {
  var lastView = rootHlp.getLastSaleView();
  var isRefundByOrder = (rootCtx.get("IS_REFUND_BY_ORDER") == "true");
  if(lastView == null) {
    lastView = rootHlp.getCurrentView();
  }

  if((lastView == null)&& (isRefundByOrder != true)) {
    return false;
  }

 
  var view = new XML(lastView);
  var isRefund = view.@transactionKind == TRANS_KIND_REFUND;
  var isBillableSale = ((view.ItemTenderView.(code == TENDER_KIND_BILLABLEREFUND)).length() != 0);
  var isOverring = ((Number(view.@saleStatus) == SALE_STATUS_LAST_SALE_VOIDED) || (Number(view.@saleStatus) == SALE_STATUS_CURRENT_SALE_VOIDED));     
  var isUberEats = false;
  
  rootCtx.set("IS_REFUND_BY_ORDER", "false");
  
  //Workaround for Kiosk Receipts
  try {
    isUberEats = isUberEatsOrder(true);
  }
  catch (e) {
  }

  try {
    var ctx=new SessionContext;
  }
  catch (e) {
    return false;
  }

  var cashlessData = String(ctx.get("CASHLESS"));
  var lines = String(cashlessData).split("CASHLESS:");
  var needsSignature = false;
  var mustSign = false;

  for(var i = 1; i < lines.length; i++) {
    var fields = String(lines[i]).split("|");
    var needsSignature = fields[5];
    mustSign = fields[17].toLowerCase() == "must sign";
    if(mustSign) {
      break;
    }
  }

      var shouldPrintSecondCopy = (
      isOverring ||
	  isRefundByOrder ||
	  (isRefund && (view.ItemTenderView.(cat == "TENDER_ELECTRONIC_PAYMENT").length() == 0)) ||	  
      (isBillableSale && !isRefund && !isUberEats) ||
      (Number(needsSignature) || isRefund || mustSign)
    );
  
  return shouldPrintSecondCopy;
}
