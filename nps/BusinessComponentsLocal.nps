/** 
 * @brief Set UberEats sale type based on the UberSaleTypeMode configuration parameter.
 * @since UberEats NVS-6999/NVS-7199 - msilva - UberEats Integration 
 */
function UberEatsSetSaleType(){
	//-1 - No change; 0 - EATIN; 1 - TAKEOUT; 2 - OTHER
	var uberSaleTypeMode = rootHlp.findParamInSectionConfig("UberSaleTypeMode","Cash");
	
	switch(uberSaleTypeMode)
	{
		case "-1": //POS behavior
			return true;
		case "0": //Eat In
			PosSetSaleType("0");
			break;
		case "1": //Take Out
			PosSetSaleType("1");
			break;
		case "2": //Others
			PosSetSaleType("2");
			break;
		default: //Default: Take Out
			PosSetSaleType("1");
			break;
	}
	
	return true;
}

