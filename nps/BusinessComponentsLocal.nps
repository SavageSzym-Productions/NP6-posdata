// Felipe Ramas - NVS-4118 - Implement Table Service Model Specifications
function PosSetVariableTagID(type, value) {

	var variableTagID = rootCtx.get("VariableTagID");
	var variableTagIDNumber = Number(variableTagID);
	var maxPuck = rootHlp.findParamInSectionConfig("tableService.puck.maxNumberValue","UserInterface");
	var lenmaxPuck = maxPuck.length;

	var lenVariableTagID = 0;
	if (variableTagID != null) {
		lenVariableTagID = variableTagID.length;
	}
	else {
		variableTagID = "";
	}
	
	/* check value to make sure is not greater than three digits */ 
	if ((type==0)) {
		variableTagID = "";
		rootCtx.set("VariableTagID",variableTagID);
		return;
	}
	
	if (value == null) return;
	variableTagID = variableTagID + value;
	if (lenVariableTagID < lenmaxPuck) {
		rootCtx.set("VariableTagID",variableTagID);
	}
}

// Felipe Ramas - NVS-4118 - Implement Table Service Model Specifications
function PosAssociateTagIdJS (TagId){

	var variableTagID = rootCtx.get("VariableTagID");
	var maxPuck = Number(rootHlp.findParamInSectionConfig("tableService.puck.maxNumberValue","UserInterface"));
	var lenVariableTagID = 0;
	//NVS-7385 17-APR-2017 Tishin
	var curView = rootHlp.getCurrentView();
	var view = new XML(curView);
	var odrSrc=view.@orderSrc;
	if ((variableTagID == null || variableTagID == "") && TagId > 0) {
		//Associate static tag Id from button on float screen (WF_AssociateTagIdFloatScreen @TagId) .
		PosAssociateTagId(TagId);
		return true;
	}
	
	// Lindomar Araujo - 2016/06/04 - NVS-4521 TagId problem when typing in a key pad and in the button pre-defined
	if (variableTagID > 0 && TagId > 0) {
		if (TagId != variableTagID) {
			PosAssociateTagId(TagId);
			rootCtx.set("VariableTagID","");
			return true;
		}
	}
	
	if (variableTagID != null) {
		lenVariableTagID = variableTagID.length;
	}
	else {
		variableTagID = "";
	}
	
	if (odrSrc !=1 && lenVariableTagID == 0 && TagId < 1) {
		PosShowMessage("MSG_TAGID_NOT_BE_EMPTY");
		rootCtx.set("VariableTagID","");
		return false;
	}

	var variableTagIDNumber = Number(variableTagID);
	if (variableTagIDNumber == 0 && odrSrc !=1 ) {
		// Felipe Ramas - change message
		//Do not allow tag Id = 0
		PosShowMessage("MSG_TAGID_GREATER_THAN_ZERO");
		rootCtx.set("VariableTagID","");
		return false;
	}
	
	if (variableTagIDNumber > maxPuck) {
		//Tag Id must be smaller or equal max puck configuration parameter.
		PosShowMessage("MSG_TAGID_MAX_SELECTED",maxPuck);
		rootCtx.set("VariableTagID","");
		return false;
	}
	
	//Associate typed tag id to the order
	PosAssociateTagId(variableTagID);	
	
	//NVS-6010 29-NOV-2016 John Brancaleon
	
	if(variableTagIDNumber == -3) { 
		//No table service selected at Kiosk
		if(view.@tableTagId == "-3") { 
			view.@tableTagId = ""; 
			view.@tableServiceMode = ""; 
		} 
		PosAssociateTagId(""); 
	}  	
	
	if(odrSrc == 1 && variableTagID < 0){
		PosAssociateTagId(""); 
  }
  
  if(variableTagIDNumber != -3) { 
    PosAssociateTagId(variableTagID);
	}
	return true;
}

