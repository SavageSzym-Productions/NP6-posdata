 /** 
 * @brief Returns the product name from a given product code
 * @param productCode The product code which we want the product name
 * @param languageName The language where we're going to find in the names-db
 * @param nameType The name type that will be returned 
 * @return product name
 * Since NVS-6852 - msilva - Product name is not displayed on the Mobile Order Recall screen when product is inactive
 */
function getProductName(productCode, languageName, nameType2) {

    var language, productName;
    var name = "";

	var strProductName = API.getNamesdbProductName(productCode, languageName); //NVS-4813
	
	if(strProductName == undefined || strProductName == null || strProductName == "")
	{
		strProductName = API.getNamesdbProductName(productCode, languageName, "INACTIVE");
	}
	
	if (null == strProductName){
		return "";
	}
	productName = new XML(strProductName);

    switch(parseInt(nameType2)) {
        case ProductNameType2.ShortName:
            name = productName.ShortName;
            break;
        case ProductNameType2.LongName:
            name = productName.LongName;
            break;
        case ProductNameType2.AlternativeName:
            name = productName.AlternativeName;
            break;
        case ProductNameType2.DTName:
        default:
            name = productName.DTName;
            break;
    }
    
    return name;
} 

