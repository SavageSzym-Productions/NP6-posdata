	return result;
}

/**
 * $Author: Marcelo Silva - NVS-5502
 * Date: 11/11/2016
 * This method checks the current product being ordered. 
 * When it is a CCM product this function automatically remove the necessary previous a la carte items added to the order.
 * The removed items will be added automatically by CCM
 * Requisites: CCM configured with DefaultProduct and DefaultQuantity in the Choice
 * @return - true: Previous a la carte choices have being removed from the order
 * 			false: No previous a la carte choices have being removed from the order
*/
function CheckCCMDefaultComponentsJS(productCode){
	
	var product = new XML(API.getProductdbProduct(productCode));

	//Check if the product is a CCM auto bundling 
	var isCCM = false;
	if(product.Tags != undefined && product.Tags != null){ 
		for each (tag in product.Tags.Tag)
		{
			if(tag == "CUSTOMER_CHOICE_MEAL")
			{
				isCCM = true;
			}
		}
	}
	if(!isCCM)
	{
		return false;
	}
	
	//Check if the product contains choices
	if(product.Choices == undefined || product.Choices == null){
		return false;
	}	
	
	//Check if order contains items
	var view = hlp.getCurrentView();
	if(view == undefined || view == null){
		return false;
	}
	view = new XML(view);
	if(view.ItemView == undefined || view.ItemView == null || view.ItemView.length() == 0){
		return false;
	}

	for each (choice in product.Choices.Component){
		//Check product default choices them remove previous a la carte items added to the order.
		//The removed products are automatically added in the CCM auto bundling.
		if(choice.DefaultProduct != undefined && choice.DefaultProduct != null){
			var choiceProductCode = choice.ProductCode;
			var maxQuantity = choice.MaxQuantity;
			var defaultProductCode = choice.DefaultProduct;
			var defaultQuantity = Number(choice.DefaultQuantity);
			
			//Check if the view already contains the product
			var toRemove = 0;
			for each(itemView in view.ItemView){
				
				if(itemView.productCode.toString() == defaultProductCode.toString() && itemView.level.toString() == "0"){
					toRemove = toRemove + Number(itemView.quantity);
				}
			}
			
			//Delete previous a la carte items from the order
			if(defaultQuantity >= toRemove)
			{
				PosDoQuantum(-1);
				PosDoQuantum(0);
				PosDoSale(defaultProductCode);
				
			}else{
				PosDoQuantum(-1);
				PosDoQuantum(toRemove - defaultQuantity);
				PosDoSale(defaultProductCode);
			}
		}else{
			return false;
		}
	}	
	
	return true;
}



