function RepEnablePrintFilterJS(change) {
	const reportType=["on","off"];
	const properties=[
		"bitmap|YES.png,bitmapdn|YES.png,title|Enable \nPrint\non",
		"bitmap|NO.png,bitmapdn|NO.png,title|Enable \nPrint\noff"
	];
	var i=0;
