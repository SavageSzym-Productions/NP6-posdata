		return false;
	}
	if (GLOBAL_CSO_IS_BLOCK) {
		GLOBAL_CSO_IS_BLOCK = false;
		sndUnBlock();
	}	
    return true;
	
    function sndBlock(msgCode) {
	  var hlp	= new BusinessObjectHelper;
	  var msgText = hlp.getSysMessage(msgCode);					
	  
	  PosSendUICmd(createBlockUnblock("UI_BLOCK", msgCode, msgText));
