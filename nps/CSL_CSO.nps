    var enterTitleTag = new XML("<EnterTitle>CSO_COUPON_PROMOTION_LBL_ENTER</EnterTitle>");
    var workflowTag = new XML("<Workflow>WF_NGK_GET_PROMOTION_ID_BY_BARCODE</Workflow>");
    
	var promotionTag = new XML("<Promotion />");
