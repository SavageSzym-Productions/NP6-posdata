 */
function PosInsertUniqueJS(arrayUnique, item) {
	var bFound=false;
	for (var iIndex=0; iIndex<arrayUnique.length; iIndex ++) {
		if(item==arrayUnique[iIndex])
		{
			bFound=true;
			break;
		}
	}
	if(!bFound) {
		arrayUnique.push(item);
	}
}

