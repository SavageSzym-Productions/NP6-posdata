function PosCSODoShowPreviousScreenJS(){
	if(csoScreensVisited.length > 0){
		if(csoScreensVisited.length > 1 && csoScreensVisited[csoScreensVisited.length - 1] == csoCurrentState.currentScreen.number){
			csoScreensVisited.pop();
