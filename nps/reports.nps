	function getExtendedSalesAmount(feeOrDeposit) {
		pdTotal = 0;
		pdList =(feeOrDeposit == "Fee")?view.Fee:view.Deposit;
		for each (var pdItem in pdList) { pdTotal = pdTotal + Number(pdItem.@value); }
