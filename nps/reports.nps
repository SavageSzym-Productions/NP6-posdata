				//NVS-5880 09-NOV-2016 John Brancaleon - right justify the amount for total savings
				line = API.geti18nMsg(1, "MSG_BC_OTHER_SAVINGS", []); 
				addLine();
				addLine(line + API.setOnRight(otherSavings.toString(), COLS-line.length));
			} 
		} 
	}

