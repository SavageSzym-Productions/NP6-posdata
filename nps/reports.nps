/** Parses 'val' to an integer, returning 0 if null or empty*/
function toInt(val)
{
	if(!val || String(val) == "") {
		return 0;
	}
	// Must create a Number first because parseInt will resolve "010" as an octal number
	return parseInt(Number(val));
}
