			feeType 	=String(feeList.(@feeCode==(i+1)).Pricing.(@priceCode=="EATIN").@type );
			feeValue 	=String(feeList.(@feeCode==(i+1)).Pricing.(@priceCode=="EATIN").Value );
			if (feeType == "RATE") {
